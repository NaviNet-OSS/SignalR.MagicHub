    </ItemGroup>
    <Message Text="solutiondir is $(SolutionDir)" />
    <!-- copy all dependent files into lib directory for execution -->
    <Copy SourceFiles="@(DependentDlls)" OverwriteReadOnlyFiles="True" DestinationFolder="$(SolutionDir)\lib" />
